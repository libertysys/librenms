    SYNTAX      Unsigned32
    UNITS       "seconds"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "'Valid lifetime' in seconds of assigned prefix
         (see 10 of RFC-3633)."
    ::= { ip6DhcpClientPrefixEntry 7 }

ip6DhcpClientPrefixFwdIfIndex OBJECT-TYPE
    SYNTAX      InterfaceIndex
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Together with 'ip6DhcpClientPrefixFwdAddress', tells next hop for
         forwarding IPv6-frames targeted to this prefix. This info equals to
         target of related IA_PD option.
         See also description for 'ip6DhcpPrefixPoolAutoRoutePrio'.
         Warning: Do not confuse forwarding-interface (chosen by server) with
         on-link-interface (chosen by client)."
    ::= { ip6DhcpClientPrefixEntry 8 }

ip6DhcpClientPrefixFwdAddress OBJECT-TYPE
    SYNTAX      InetAddressIPv6
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "See description for 'ip6DhcpClientPrefixFwdIfIndex'."
    ::= { ip6DhcpClientPrefixEntry 9 }

ip6DhcpClientPrefixAge OBJECT-TYPE
    SYNTAX      TimeStamp
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Age of this prefix-entry."
    ::= { ip6DhcpClientPrefixEntry 10 }

END
