    SEQUENCE {
        ip6DhcpSntpServerOptId              INTEGER,
        ip6DhcpSntpServerOptAdminStatus     INTEGER,
        ip6DhcpSntpServerOptDescription     DisplayString,
        ip6DhcpSntpServerOptOptionSetId     INTEGER,
        ip6DhcpSntpServerOptListIdx         INTEGER,
        ip6DhcpSntpServerOptAddress         InetAddressIPv6
    }

ip6DhcpSntpServerOptId OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Internal identifier of this SNTP-entry."
    ::= { ip6DhcpSntpServerOptEntry 1 }

ip6DhcpSntpServerOptAdminStatus OBJECT-TYPE
    SYNTAX      INTEGER {
        enabled(1),
        disabled(2),
        delete(3)
    }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Activate/deactivate/delete this SNTP-entry."
    DEFVAL      { enabled }
    ::= { ip6DhcpSntpServerOptEntry 2 }

ip6DhcpSntpServerOptDescription OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Textual description of this SNTP-entry."
    ::= { ip6DhcpSntpServerOptEntry 3 }

ip6DhcpSntpServerOptOptionSetId OBJECT-TYPE
    SYNTAX      INTEGER (1..65535)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "'Id' value of related 'ip6DhcpOptionSet' entry.
         If no 'ip6DhcpOptionSetId' matches, this SNTP-entry is ignored."
    ::= { ip6DhcpSntpServerOptEntry 4 }

ip6DhcpSntpServerOptListIdx OBJECT-TYPE
    SYNTAX      INTEGER (0..255)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Defines sorting-order in case of several matching SNTP-entries.
         Lower number means 'earlier in list'."
    ::= { ip6DhcpSntpServerOptEntry 5 }

ip6DhcpSntpServerOptAddress OBJECT-TYPE
    SYNTAX      InetAddressIPv6
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Address of SNTP-server."
    ::= { ip6DhcpSntpServerOptEntry 6 }

