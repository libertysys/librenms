-- Administrative: Generic stateless DHCPv6-options

ip6DhcpGenericOptTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF Ip6DhcpGenericOptEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "With this table, some stateless DHCPv6-options with fixed values can
         be specified rather generically.
         This may be useful for configuring such options that lack dedicated
         support by current DHCPv6-code and that may even be completely
         proprietary or vendor-specific.
         However, great care should be taken not to specify any options here
         that may interfere with any 'regular' ones, i.e. such that are
         automatically handled by current DHCPv6-code or that are configured
         by some dedicated, option-code-specific tables.
         At present, all (matching) configured generic options are put into
         related Advertise/Reply messages even if their option-codes have not
         been requested by Option Request Option (see 22.7 of RFC-3315).
         Each matching generic option results in a separate option-entry (in
         arbitrary order) even if sharing same option-code."
    ::= { dhcp6MIB 4 }

ip6DhcpGenericOptEntry OBJECT-TYPE
    SYNTAX      Ip6DhcpGenericOptEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        ""
    INDEX       { ip6DhcpGenericOptId }
    ::= { ip6DhcpGenericOptTable 1 }

Ip6DhcpGenericOptEntry ::=
    SEQUENCE {
        ip6DhcpGenericOptId             INTEGER,
        ip6DhcpGenericOptAdminStatus    INTEGER,
        ip6DhcpGenericOptDescription    DisplayString,
        ip6DhcpGenericOptOptionSetId    INTEGER,
        ip6DhcpGenericOptCode           INTEGER,
        ip6DhcpGenericOptData           OCTET STRING
    }

ip6DhcpGenericOptId OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Internal identifier of this option-entry."
    ::= { ip6DhcpGenericOptEntry 1 }

ip6DhcpGenericOptAdminStatus OBJECT-TYPE
    SYNTAX      INTEGER {
        enabled(1),
        disabled(2),
        delete(3)
    }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Activate/deactivate/delete this option-entry."
    DEFVAL      { enabled }
    ::= { ip6DhcpGenericOptEntry 2 }

ip6DhcpGenericOptDescription OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Textual description of this option-entry."
    ::= { ip6DhcpGenericOptEntry 3 }

ip6DhcpGenericOptOptionSetId OBJECT-TYPE
    SYNTAX      INTEGER (1..65535)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "'Id' value of related 'ip6DhcpOptionSet' entry.
         If no 'ip6DhcpOptionSetId' matches, this option-entry is ignored."
    ::= { ip6DhcpGenericOptEntry 4 }

ip6DhcpGenericOptCode OBJECT-TYPE
    SYNTAX      INTEGER (0..65535)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "DHCPv6-option-code of this entry (see 22.1 of RFC-3315).
         Please read warning in general description of this table about not
         specifying any option-codes here that may interfere with 'regular'
         options-processing."
    ::= { ip6DhcpGenericOptEntry 5 }

ip6DhcpGenericOptData OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Data-string that is pasted into data-part of this option-entry
         (see 22.1 of RFC-3315)."
    ::= { ip6DhcpGenericOptEntry 6 }

---------------------------------------------------------------------------
-- Administrative: DHCPv6-prefix-pools for IA_PDs

ip6DhcpPrefixPoolTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF Ip6DhcpPrefixPoolEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "DHCPv6-prefix-pools for filling requested IA_PD options
         (see 9 and 10 of RFC-3633)."
    ::= { dhcp6MIB 5 }

ip6DhcpPrefixPoolEntry OBJECT-TYPE
    SYNTAX      Ip6DhcpPrefixPoolEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        ""
    INDEX       { ip6DhcpPrefixPoolId }
    ::= { ip6DhcpPrefixPoolTable 1 }

Ip6DhcpPrefixPoolEntry ::=
    SEQUENCE {
        ip6DhcpPrefixPoolId                 INTEGER,
        ip6DhcpPrefixPoolAdminStatus        INTEGER,
        ip6DhcpPrefixPoolDescription        DisplayString,
        ip6DhcpPrefixPoolOptionSetId        INTEGER,
        ip6DhcpPrefixPoolPrefixId           INTEGER,
        ip6DhcpPrefixPoolSubPrefixLength    INTEGER,
        ip6DhcpPrefixPoolStartOffset        InetAddressIPv6,
        ip6DhcpPrefixPoolStartPrefix        InetAddressIPv6,
        ip6DhcpPrefixPoolEndOffset          InetAddressIPv6,
        ip6DhcpPrefixPoolEndPrefix          InetAddressIPv6,
        ip6DhcpPrefixPoolSize               Counter64,
        ip6DhcpPrefixPoolPrefLifetime       Unsigned32,
        ip6DhcpPrefixPoolValidLifetime      Unsigned32,
        ip6DhcpPrefixPoolAutoRoutePrio      INTEGER,
        ip6DhcpPrefixPoolOpPrefLifetime     Unsigned32,
        ip6DhcpPrefixPoolOpValidLifetime    Unsigned32
    }

ip6DhcpPrefixPoolId OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Internal identifier of this prefix-pool."
    ::= { ip6DhcpPrefixPoolEntry 1 }

ip6DhcpPrefixPoolAdminStatus OBJECT-TYPE
    SYNTAX      INTEGER {
        enabled(1),
        disabled(2),
        delete(3)
    }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Activate/deactivate/delete this prefix-pool."
    DEFVAL      { enabled }
    ::= { ip6DhcpPrefixPoolEntry 2 }

ip6DhcpPrefixPoolDescription OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Textual description of this prefix-pool."
    ::= { ip6DhcpPrefixPoolEntry 3 }

ip6DhcpPrefixPoolOptionSetId OBJECT-TYPE
    SYNTAX      INTEGER (1..65535)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "'Id' value of related 'ip6DhcpOptionSet' entry.
         If no 'ip6DhcpOptionSetId' matches, this prefix-pool is ignored."
    ::= { ip6DhcpPrefixPoolEntry 4 }

ip6DhcpPrefixPoolPrefixId OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "'Id' value of related 'ip6Prefix' entry.
         Prefixes from this pool are assigned only if related 'ip6Prefix'
         entry exists and if its 'OperStatus' is not 'down'.
         If multiple pools refer to same 'ip6Prefix' then their ranges
         must not overlap (see 'ip6DhcpPrefixPoolStartOffset' and
         'ip6DhcpPrefixPoolEndOffset').
         If any 'ip6LinkPrefix' entries refer to same 'ip6Prefix' then their
         sub-prefixes must not overlap with prefix-range of this pool.
         'IfIndex' of related 'ip6Prefix' entry must be zero (unspecified)
         because DHCPv6-clients may freely link-assign their prefixes.
         'OnLinkFlag' of related 'ip6LinkPrefix' entry must be 'false'.
         'AutonomousFlag' of related 'ip6LinkPrefix' entry must be 'false'."
    ::= { ip6DhcpPrefixPoolEntry 5 }

ip6DhcpPrefixPoolSubPrefixLength OBJECT-TYPE
    SYNTAX      INTEGER (0..64)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Length of generated sub-prefixes, must be <= 64 and >= related
         'ip6PrefixLength'."
    ::= { ip6DhcpPrefixPoolEntry 6 }

ip6DhcpPrefixPoolStartOffset OBJECT-TYPE
    SYNTAX      InetAddressIPv6
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Together with 'Prefix' of related 'ip6Prefix' entry, specifies
         lowest assignable prefix in this pool.
         Result of prefix plus offset is stored in 'StartPrefix' field.
         Lowest (128-'SubPrefixLength') bits of offset must be zero.
         Offset must be smaller than 2^(128-L) where L is 'Length' of related
         'ip6Prefix' entry."
    ::= { ip6DhcpPrefixPoolEntry 7 }

ip6DhcpPrefixPoolStartPrefix OBJECT-TYPE
    SYNTAX      InetAddressIPv6
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Lowest assignable prefix in this pool,
         or 0 if no suitable 'ip6Prefix' entry was found.
         See description of 'StartOffset' field for further details."
    ::= { ip6DhcpPrefixPoolEntry 8 }

ip6DhcpPrefixPoolEndOffset OBJECT-TYPE
    SYNTAX      InetAddressIPv6
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Together with 'Prefix' of related 'ip6Prefix' entry, specifies
         highest assignable prefix in this pool.
         Result of prefix plus offset is stored in 'EndPrefix' field.
         Lowest (128-'SubPrefixLength') bits of offset must be zero.
         Offset must be smaller than 2^(128-L) where L is 'Length' of related
         'ip6Prefix' entry.
         Offset must not be smaller than related 'StartOffset'."
    ::= { ip6DhcpPrefixPoolEntry 9 }

ip6DhcpPrefixPoolEndPrefix OBJECT-TYPE
    SYNTAX      InetAddressIPv6
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Highest assignable prefix in this pool,
         or 0 if no suitable 'ip6Prefix' entry was found.
         See description of 'EndOffset' field for further details."
    ::= { ip6DhcpPrefixPoolEntry 10 }

ip6DhcpPrefixPoolSize OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Number of prefixes in this pool, calculated by formula
         1+(('EndPrefix'-'StartPrefix')/(2^(128-'SubPrefixLength'))),
         or 0 if no suitable 'ip6Prefix' entry was found.
         Warning: Due to 64-bit-restriction of this field, the maximum
         possible pool-size of 2^64 is also represented by a 0 value."
    ::= { ip6DhcpPrefixPoolEntry 11 }

ip6DhcpPrefixPoolPrefLifetime OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "seconds"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Maximum 'preferred lifetime' in seconds of prefixes in this pool.
         More details are described for 'ip6DhcpPrefixPoolOpPrefLifetime'."
    DEFVAL      { 43200 }
    ::= { ip6DhcpPrefixPoolEntry 12 }

ip6DhcpPrefixPoolValidLifetime OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "seconds"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Maximum 'valid lifetime' in seconds of prefixes in this pool.
         More details are described for 'ip6DhcpPrefixPoolOpValidLifetime'."
    DEFVAL      { 86400 }
    ::= { ip6DhcpPrefixPoolEntry 13 }

ip6DhcpPrefixPoolAutoRoutePrio OBJECT-TYPE
    SYNTAX      INTEGER (-1..16)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "'Priority' value of related, auto-generated 'ip6AdmRoute' entries.
         A value of -1 means NO auto-generated 'ip6AdmRoute' entries.
         See also description for 'ip6DhcpClientPrefixFwdIfIndex' and
         'ip6DhcpClientPrefixFwdAddress'."
    DEFVAL      { 8 }
    ::= { ip6DhcpPrefixPoolEntry 14 }

ip6DhcpPrefixPoolOpPrefLifetime OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "seconds"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "'Preferred lifetime' in seconds of prefixes in this pool
         (see 10 of RFC-3633). Value is set to minimum of configured
         maximum 'ip6DhcpPrefixPoolPrefLifetime' and inherited
         'ip6PrefixAdvPreferredLifetime'."
    DEFVAL      { 43200 }
    ::= { ip6DhcpPrefixPoolEntry 15 }

ip6DhcpPrefixPoolOpValidLifetime OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "seconds"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "'Valid lifetime' in seconds of prefixes in this pool
         (see 10 of RFC-3633). Value is set to minimum of configured
         maximum 'ip6DhcpPrefixPoolValidLifetime' and inherited
         'ip6PrefixAdvValidLifetime'."
    DEFVAL      { 86400 }
    ::= { ip6DhcpPrefixPoolEntry 16 }

---------------------------------------------------------------------------
-- Administrative: Static interface-IDs for DHCPv6-assigned addresses

ip6DhcpStaticIfIdentTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF Ip6DhcpStaticIfIdentEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Interface-IDs (least significant 64 bits, see 2.5.1 of RFC-4291) for
         semi-statically assigned 'ip6DhcpClientAddress' entries (find more
         details on address-building there).
         Related DHCPv6-client is specified by 'ClientDUID' of related
         'ip6DhcpOptionSet' entry.
         Warning: Clients with static interface-IDs do not get any other
         addresses than such ending with one of their specified interface-IDs,
         making any address-conflicts more critical. Additionally, if clients
         with static interface-IDs request multiple IAs on the same link
         because they have multiple interfaces to it, only the first IA will
         receive the related addresses."
    ::= { dhcp6MIB 6 }

ip6DhcpStaticIfIdentEntry OBJECT-TYPE
    SYNTAX      Ip6DhcpStaticIfIdentEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        ""
    INDEX       { ip6DhcpStaticIfIdentId }
    ::= { ip6DhcpStaticIfIdentTable 1 }

Ip6DhcpStaticIfIdentEntry ::=
    SEQUENCE {
        ip6DhcpStaticIfIdentId              INTEGER,
        ip6DhcpStaticIfIdentAdminStatus     INTEGER,
        ip6DhcpStaticIfIdentDescription     DisplayString,
        ip6DhcpStaticIfIdentOptionSetId     INTEGER,
        ip6DhcpStaticIfIdentIdentifier      InetAddressIPv6
    }

ip6DhcpStaticIfIdentId OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Internal identifier of this static interface-ID-entry."
    ::= { ip6DhcpStaticIfIdentEntry 1 }

ip6DhcpStaticIfIdentAdminStatus OBJECT-TYPE
    SYNTAX      INTEGER {
        enabled(1),
        disabled(2),
        delete(3)
    }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Activate/deactivate/delete this static interface-ID-entry."
    DEFVAL      { enabled }
    ::= { ip6DhcpStaticIfIdentEntry 2 }

ip6DhcpStaticIfIdentDescription OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Textual description of this static interface-ID-entry."
    ::= { ip6DhcpStaticIfIdentEntry 3 }

ip6DhcpStaticIfIdentOptionSetId OBJECT-TYPE
    SYNTAX      INTEGER (1..65535)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "'Id' value of related 'ip6DhcpOptionSet' entry.
         If no 'ip6DhcpOptionSetId' matches, this entry is ignored.
         'ClientDUID' must be specified in related 'ip6DhcpOptionSet' entry
         in order to avoid that same address is assigned to multiple clients."
    ::= { ip6DhcpStaticIfIdentEntry 4 }

ip6DhcpStaticIfIdentIdentifier OBJECT-TYPE
    SYNTAX      InetAddressIPv6
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Static interface-ID-value (see 2.5.1 of RFC-4291)."
    ::= { ip6DhcpStaticIfIdentEntry 5 }

---------------------------------------------------------------------------
-- Administrative: Statically DHCPv6-assigned client-FQDNs

ip6DhcpClientFqdnOptTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF Ip6DhcpClientFqdnOptEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Statically assigned client-FQDNs (see 4 of RFC-4704).
         If router is configured as DNS-server then related (name, address)
         entries are added also into DNS-cache, independently from whether
         FQDN-option is requested or not.
         In case of FQDN-request, at most one entry may be replied. Thus,
         following rule applies. If entry with empty 'Name' matches and if
         client provides own name then it is simply confirmed. Otherwise,
         'first' matching one is taken according to sorting-order defined via
         'ip6DhcpClientFqdnOptListIdx'."
    ::= { dhcp6MIB 7 }

ip6DhcpClientFqdnOptEntry OBJECT-TYPE
    SYNTAX      Ip6DhcpClientFqdnOptEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        ""
    INDEX       { ip6DhcpClientFqdnOptId }
    ::= { ip6DhcpClientFqdnOptTable 1 }

Ip6DhcpClientFqdnOptEntry ::=
    SEQUENCE {
        ip6DhcpClientFqdnOptId              INTEGER,
        ip6DhcpClientFqdnOptAdminStatus     INTEGER,
        ip6DhcpClientFqdnOptDescription     DisplayString,
        ip6DhcpClientFqdnOptOptionSetId     INTEGER,
        ip6DhcpClientFqdnOptListIdx         INTEGER,
        ip6DhcpClientFqdnOptName            DisplayString,
        ip6DhcpClientFqdnOptFullName        DisplayString
    }

ip6DhcpClientFqdnOptId OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Internal identifier of this FQDN-entry."
    ::= { ip6DhcpClientFqdnOptEntry 1 }

ip6DhcpClientFqdnOptAdminStatus OBJECT-TYPE
    SYNTAX      INTEGER {
        enabled(1),
        disabled(2),
        delete(3)
    }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Activate/deactivate/delete this FQDN-entry."
    DEFVAL      { enabled }
    ::= { ip6DhcpClientFqdnOptEntry 2 }

ip6DhcpClientFqdnOptDescription OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Textual description of this FQDN-entry."
    ::= { ip6DhcpClientFqdnOptEntry 3 }

ip6DhcpClientFqdnOptOptionSetId OBJECT-TYPE
    SYNTAX      INTEGER (1..65535)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "'Id' value of related 'ip6DhcpOptionSet' entry.
         If no 'ip6DhcpOptionSetId' matches, this FQDN-entry is ignored.
         'ClientDUID' must be specified in related 'ip6DhcpOptionSet' entry
         in order to avoid that same FQDN is assigned to multiple clients."
    ::= { ip6DhcpClientFqdnOptEntry 4 }

ip6DhcpClientFqdnOptListIdx OBJECT-TYPE
    SYNTAX      INTEGER (0..255)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Defines sorting-order in case of several matching FQDN-entries.
         Lower number means 'earlier in list'."
    ::= { ip6DhcpClientFqdnOptEntry 5 }

ip6DhcpClientFqdnOptName OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Fully qualified domain-name, or empty.
         If empty, 'Name' field of related 'ip6DhcpStatefulClient' is used
         instead, if not also empty.
         At most one entry with empty 'Name' should be specified per client.
         Fully qualified domain-names must have trailing dot for distinction
         from relative/partial domain-names. However, many users/admins tend
         to forget this. Thus, 'proper version' of this field is stored in
         field 'FullName'."
    ::= { ip6DhcpClientFqdnOptEntry 6 }

ip6DhcpClientFqdnOptFullName OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "'Proper' fully qualified domain-name, or empty.
         See description of field 'Name'."
    ::= { ip6DhcpClientFqdnOptEntry 7 }

---------------------------------------------------------------------------
-- Administrative: Statically DHCPv6-assigned SNTP-servers

ip6DhcpSntpServerOptTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF Ip6DhcpSntpServerOptEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Statically assigned SNTP-server-addresses (see 4 of RFC-4075).
         If multiple entries match then all related addresses are stored in
         a single SNTP-server-option according to sorting-order specified via
         'ip6DhcpSntpServerOptListIdx'.
         If SNTP-option is requested but no entries match then DHCP-server
         returns own address if NTP-service of router is activated."
    ::= { dhcp6MIB 8 }

ip6DhcpSntpServerOptEntry OBJECT-TYPE
    SYNTAX      Ip6DhcpSntpServerOptEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        ""
    INDEX       { ip6DhcpSntpServerOptId }
    ::= { ip6DhcpSntpServerOptTable 1 }

