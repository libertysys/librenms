-- Operative: DHCPv6-clients with assigned addresses/prefixes

ip6DhcpStatefulClientTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF Ip6DhcpStatefulClientEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "DHCPv6-clients with currently assigned address, prefix, or FQDN."
    ::= { dhcp6MIB 11 }

ip6DhcpStatefulClientEntry OBJECT-TYPE
    SYNTAX      Ip6DhcpStatefulClientEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        ""
    INDEX       { ip6DhcpStatefulClientDUID }
    ::= { ip6DhcpStatefulClientTable 1 }

Ip6DhcpStatefulClientEntry ::=
    SEQUENCE {
        ip6DhcpStatefulClientDUID           OCTET STRING,
        ip6DhcpStatefulClientName           DisplayString,
        ip6DhcpStatefulClientAge            TimeStamp,
        ip6DhcpStatefulClientLastUpdate     TimeStamp
    }

ip6DhcpStatefulClientDUID OBJECT-TYPE
    SYNTAX  OCTET STRING
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "DUID of DHCPv6-client (see 9 and 22.2 of RFC-3315).
         An empty dummy-DUID is used for storing addresses from received
         Decline-messages (see section 5.3 of RFC-33)."
    ::= { ip6DhcpStatefulClientEntry 1 }

ip6DhcpStatefulClientName OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Partial or fully qualified domain-name, if provided by client,
         otherwise empty. Value is overridden by 'Name' of related
         'ip6DhcpClientFqdnOpt' entry, if existing and not empty."
    ::= { ip6DhcpStatefulClientEntry 2 }

ip6DhcpStatefulClientAge OBJECT-TYPE
    SYNTAX      TimeStamp
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Age of this client-entry."
    ::= { ip6DhcpStatefulClientEntry 3 }

ip6DhcpStatefulClientLastUpdate OBJECT-TYPE
    SYNTAX      TimeStamp
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Timestamp for latest update of this client-entry."
    ::= { ip6DhcpStatefulClientEntry 4 }

---------------------------------------------------------------------------
-- Operative: Currently DHCPv6-assigned IA_NA-addresses

ip6DhcpClientAddressTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF Ip6DhcpClientAddressEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Currently assigned client-addresses, built for each pair of requested
         IA_NA and matching 'ip6LinkPrefix' entry.
         For clients with semi-statically assigned addresses, the interface-ID
         part (least significant 64 bits, see section 2.5.1 of RFC-4291) is
         taken from their 'ip6DhcpStaticIfIdent' entries.
         For all other clients, some random interface-ID (excluding any static
         ones) is used unless they request some specific and valid addresses."
    ::= { dhcp6MIB 12 }

ip6DhcpClientAddressEntry OBJECT-TYPE
    SYNTAX      Ip6DhcpClientAddressEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        ""
    INDEX       { ip6DhcpClientAddressValue }
    ::= { ip6DhcpClientAddressTable 1 }

Ip6DhcpClientAddressEntry ::=
    SEQUENCE {
        ip6DhcpClientAddressValue           InetAddressIPv6,
        ip6DhcpClientAddressDUID            OCTET STRING,
        ip6DhcpClientAddressLinkPrefixId    INTEGER,
        ip6DhcpClientAddressIAID            Unsigned32,
        ip6DhcpClientAddressPrefLifetime    Unsigned32,
        ip6DhcpClientAddressValidLifetime   Unsigned32,
        ip6DhcpClientAddressAge             TimeStamp
    }

ip6DhcpClientAddressValue OBJECT-TYPE
    SYNTAX      InetAddressIPv6
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Assigned address."
    ::= { ip6DhcpClientAddressEntry 1 }

ip6DhcpClientAddressDUID OBJECT-TYPE
    SYNTAX  OCTET STRING
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "DUID of related DHCPv6-client (see 9 and 22.2 of RFC-3315).
         An empty dummy-DUID is used for storing addresses from received
         Decline-messages (see section 5.3 of RFC-33)."
    ::= { ip6DhcpClientAddressEntry 2 }

ip6DhcpClientAddressLinkPrefixId OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "'Id' value of related 'ip6LinkPrefix' entry."
    ::= { ip6DhcpClientAddressEntry 3 }

ip6DhcpClientAddressIAID OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "IAID of related IA_NA (see 22.4 of RFC-3315)."
    ::= { ip6DhcpClientAddressEntry 4 }

ip6DhcpClientAddressPrefLifetime OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "seconds"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "'Preferred lifetime' in seconds of assigned address
         (see 22.6 of RFC-3315)."
    ::= { ip6DhcpClientAddressEntry 5 }

ip6DhcpClientAddressValidLifetime OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "seconds"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "'Valid lifetime' in seconds of assigned address
         (see 22.6 of RFC-3315)."
    ::= { ip6DhcpClientAddressEntry 6 }

ip6DhcpClientAddressAge OBJECT-TYPE
    SYNTAX      TimeStamp
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Age of this address-entry."
    ::= { ip6DhcpClientAddressEntry 7 }

---------------------------------------------------------------------------
