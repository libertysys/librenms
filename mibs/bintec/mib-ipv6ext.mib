    ip6SifAliasStatus OBJECT-TYPE
        SYNTAX      INTEGER {
            enabled(1),
            disabled(2)
        }
        ACCESS      read-write
        STATUS      mandatory
        DESCRIPTION
            "Defines the administrative status of this rule."
        DEFVAL { enabled }
    ::= { ip6SifAliasEntry 6 }

    ip6SifAliasPriority OBJECT-TYPE
        SYNTAX      INTEGER {
            default(1),
            low-latency(2),
            high(3),
            medium(4),
            low(5)
        }
        ACCESS      read-write
        STATUS      mandatory
        DESCRIPTION
