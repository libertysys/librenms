        ACCESS      not-accessible
        STATUS      mandatory
        DESCRIPTION
            "Contains a IPv6 'Stateful Inspection Firewall' (SIF) policy."
    ::= { ip6Config 8 }

    ip6SifAliasEntry OBJECT-TYPE
        SYNTAX      Ip6SifAliasEntry
        ACCESS      not-accessible
        STATUS      mandatory
        DESCRIPTION
            ""
        INDEX   { ip6SifAliasOrder }
    ::= { ip6SifAliasTable 1 }

    Ip6SifAliasEntry ::= SEQUENCE {
        ip6SifAliasOrder        INTEGER,
        ip6SifAliasSource       INTEGER,
        ip6SifAliasDestination  INTEGER,
        ip6SifAliasService      INTEGER,
        ip6SifAliasAction       INTEGER,
        ip6SifAliasStatus       INTEGER,
        ip6SifAliasPriority     INTEGER,
        ip6SifAliasClassId      INTEGER,
        ip6SifAliasDscp         INTEGER
    }

    ip6SifAliasOrder OBJECT-TYPE
        SYNTAX      INTEGER
        ACCESS      read-write
        STATUS      mandatory
        DESCRIPTION
            "The configured order (implict priority) for this
             'Stateful Inspection Firewall' (SIF) rule."
