        ip6DsLiteUsedLocalAddress   InetAddressIPv6,
        ip6DsLiteUsedRemoteAddress  InetAddressIPv6,
        ip6DsLiteAftrFqdn           DisplayString
    }

    ip6DsLiteIfIndexV4 OBJECT-TYPE
        SYNTAX      InterfaceIndexOrZero
        ACCESS      read-write
        STATUS      mandatory
        DESCRIPTION
            "Unique Identifier for this entry"
    ::= { ip6DsLiteEntry 1 }

    ip6DsLiteIfIndexV6 OBJECT-TYPE
        SYNTAX      InterfaceIndex
        ACCESS      read-write
        STATUS      mandatory
        DESCRIPTION
            "Specifies the associated IPv6 interface for this entry."
