    IPV6-MIB DEFINITIONS ::= BEGIN

    IMPORTS
        MODULE-IDENTITY, OBJECT-TYPE, NOTIFICATION-TYPE, mib-2, Counter32,
        Unsigned32, Integer32, Gauge32
            FROM SNMPv2-SMI
        DisplayString, PhysAddress, TruthValue, TimeStamp, VariablePointer,
        RowPointer
            FROM SNMPv2-TC
        MODULE-COMPLIANCE, OBJECT-GROUP, NOTIFICATION-GROUP
            FROM SNMPv2-CONF
        InterfaceIndexOrZero, InterfaceIndex
            FROM IF-MIB
        InetAddressType, InetAddressIPv6, InetAddressIPv4, InetAddress
            FROM INET-ADDRESS-MIB
        IANAipRouteProtocol
            FROM IANA-RTPROTO-MIB
        IANAifType
            FROM IANAifType-MIB
        biboip
            FROM BINTEC-MIB;

    ip6MIB MODULE-IDENTITY
        LAST-UPDATED "201801100000Z"
        ORGANIZATION "bintec elmeg GmbH"
        CONTACT-INFO "EMail: info@bintec-elmeg.com
                      Web:   www.bintec-elmeg.com"
        DESCRIPTION
            "MIB module for bintec elmeg specific definitions used by entities
             implementing the IPv6 protocol."
        ::= { biboip 100 }

    ip6Config OBJECT IDENTIFIER ::= { ip6MIB 1 }
    ip6Stats  OBJECT IDENTIFIER ::= { ip6MIB 2 }
    ip6Debug  OBJECT IDENTIFIER ::= { ip6MIB 3 }


-- Start: Administrative: Global IPv6-settings

    ip6CfgGlobals OBJECT IDENTIFIER ::= { ip6Config 1 }

    ip6UniqueLocal OBJECT-TYPE
        SYNTAX      InetAddressIPv6
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The Locally Assigned Global Id for the locally assigned Prefix."
    ::= { ip6CfgGlobals 1 }

    ip6UniqueDUID OBJECT-TYPE
        SYNTAX      OCTET STRING
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The locally unique DUID used by internal DHCPv6-client and server
             (see 9, 22.2, 22.3 of RFC-3315)."
    ::= { ip6CfgGlobals 2 }

-- End: Administrative: Global IPv6-settings


-- Start: Administrative: IPv6-interfaces

   ip6AdmIfTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF Ip6AdmIfEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The IPv6 Interfaces table contains information
            on the entity's internetwork-layer interfaces.
            An IPv6 interface constitutes a logical network
            layer attachment to the layer immediately below
            IPv6 including internet layer 'tunnels', such as
            tunnels over IPv4 or IPv6 itself."
    ::= { ip6Config 2 }

    ip6AdmIfEntry OBJECT-TYPE
        SYNTAX      Ip6AdmIfEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "An interface entry containing objects
             about a particular IPv6 interface."
        INDEX   { ip6AdmIfIndex }
    ::= { ip6AdmIfTable 1 }

    Ip6AdmIfEntry ::= SEQUENCE {
        ip6AdmIfIndex               InterfaceIndex,
        ip6AdmIfIdentifier          InetAddressIPv6,
        ip6AdmIfAdvertise           INTEGER,
        ip6AdmIfPolicy              INTEGER,
        ip6AdmIfLifetime            INTEGER,
        ip6AdmIfPreference          INTEGER,
        ip6AdmIfAdminStatus         INTEGER,
        ip6AdmIfValidLifetime       INTEGER,
        ip6AdmIfMtu                 INTEGER,
        ip6AdmIfReachableTime       INTEGER,
        ip6AdmIfRetransTimer        Unsigned32,
        ip6AdmIfDhcpMode            INTEGER,
        ip6AdmIfAdvCurrHopLimit     INTEGER,
        ip6AdmIfMldMode             INTEGER,
        ip6AdmIfDhcpOffer           BITS,
        ip6AdmIfDnsAssign           INTEGER,
        ip6AdmIfAdvInterval         INTEGER,
        ip6AdmIfDhcpAddrMode        INTEGER
    }

    ip6AdmIfIndex OBJECT-TYPE
        SYNTAX      InterfaceIndex
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "A unique non-zero value identifying the particular IPv6 interface."
    ::= { ip6AdmIfEntry 1 }

    ip6AdmIfIdentifier OBJECT-TYPE
        SYNTAX      InetAddressIPv6
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The interface's address at its protocol sub-layer.  For
             example, for an 802.x interface, this object normally
             contains a MAC address.  The interface's media-specific MIB
             must define the bit and byte ordering and the format of the
             value of this object.  For interfaces which do not have such
             an address (e.g., a serial line), this object should contain
             an octet string of zero length."
    ::= { ip6AdmIfEntry 2 }

    ip6AdmIfAdvertise OBJECT-TYPE
        SYNTAX      INTEGER {
            accept(1),
            publish(2),
            ignore(3)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The handling of router advertisments.
             accept  = accept routes
             publish = server mode, send advertisements
             ignore  = do not handle router advertisements"
        DEFVAL { ignore }
    ::= { ip6AdmIfEntry 3 }

    ip6AdmIfPolicy OBJECT-TYPE
        SYNTAX      INTEGER {
            untrusted(1),
            trusted(2)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The kind of connection.
             trusted    = Secure, filtering deactivated
             untrusted  = not secure (WAN) filter activated"
        DEFVAL { untrusted }
    ::= { ip6AdmIfEntry 4 }

    ip6AdmIfLifetime OBJECT-TYPE
        SYNTAX      INTEGER (0..65535)
        UNITS       "seconds"
        MAX-ACCESS  read-create
        STATUS      obsolete
        DESCRIPTION
            "Obsoleted: Replaced by new variable ip6AdmIfAdvInterval."
        DEFVAL { 600 }
    ::= { ip6AdmIfEntry 5 }

    ip6AdmIfPreference OBJECT-TYPE
        SYNTAX      INTEGER {
            high(1),
            medium(2),
            low(3)
        }
        MAX-ACCESS read-create
        STATUS     current
        DESCRIPTION
            "Priority of the router according RFC 2461."
        DEFVAL { medium }
    ::= { ip6AdmIfEntry 6 }

    ip6AdmIfAdminStatus OBJECT-TYPE
        SYNTAX      INTEGER {
            enable(1),
            disable(2),
            delete(3)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The Administration state of the entry."
        DEFVAL { enable }
    ::= { ip6AdmIfEntry 7 }

    ip6AdmIfValidLifetime OBJECT-TYPE
        SYNTAX      INTEGER (0..65535)
        UNITS       "seconds"
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "Advertised router-lifetime in seconds for this interface.
             Relevant only if ip6AdmIfAdvertise is set to 'publish'.
             Typically, lifetime is NOT set here (zero value) but derived from
             ip6AdmIfAdvInterval instead (see AdvDefaultLifetime in RFC-4861)."
        DEFVAL { 0 }
    ::= { ip6AdmIfEntry 8 }

    ip6AdmIfMtu OBJECT-TYPE
        SYNTAX      INTEGER (0..4096)
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "Normally 0. If set this MTU will be used instead of standard MTU."
        DEFVAL { 0 }
    ::= { ip6AdmIfEntry 9 }

    ip6AdmIfReachableTime OBJECT-TYPE
        SYNTAX      INTEGER (0..3600000)
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "Defines the Value in mS of the Reachable Time
             field in the Router Advertisement (RA) message.
             0 is default and means unspecified."
        DEFVAL { 0 }
    ::= { ip6AdmIfEntry 10 }

    ip6AdmIfRetransTimer OBJECT-TYPE
        SYNTAX      Unsigned32
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "Defines the Value in mS of the Retrans Timer
             field in the Router Advertisement (RA) message.
             0 is default and means unspecified."
        DEFVAL { 0 }
    ::= { ip6AdmIfEntry 11 }

    ip6AdmIfDhcpMode OBJECT-TYPE
        SYNTAX      INTEGER {
            off(1),
            client(2),
            server(3)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "Defines DHCPv6-related behaviour on this interface."
        DEFVAL { client }
    ::= { ip6AdmIfEntry 12 }

   ip6AdmIfAdvCurrHopLimit OBJECT-TYPE
        SYNTAX      INTEGER {
            enable(1),
            disable(2)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "Enable or disable transfer of default
             Hop Limit in Router Advertisement (RA)."
        DEFVAL { enable }
    ::= { ip6AdmIfEntry 13 }

    ip6AdmIfMldMode OBJECT-TYPE
        SYNTAX      INTEGER {
            off(1),
            auto(2),
            downstream(3),
            upstream(4)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "Set the Mld Proxy Mode for this interface
                 off         disable all Mld Action
                 auto        use downstream to Link Prefix
                             upstream otherwise
                 dowstream   interface is downstream link
                 upstream    interface is upstream link"
        DEFVAL { off }
    ::= { ip6AdmIfEntry 14 }

    ip6AdmIfDhcpOffer OBJECT-TYPE
        SYNTAX      BITS {
            other(0),
            managed(1)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "'Default-values' for 'ip6IfDhcpOffer' (see further description
             there). Processing depends on current value of ip6AdmIfAdvertise.
             - accept:
                 When ip6IfOperStatus goes up, 'ip6IfDhcpOffer' bits are
                 initialised to false and later updated to M/O-flags for
                 each received router-advertisement. However, if initial
                 solicitation for router-advertisements times-out, these
                 default-values are copied to ip6IfDhcpOffer as a substitute.
             - ignore/publish:
                 When ip6IfOperStatus goes up, 'ip6IfDhcpOffer' bits are
                 initialised to these default-values."
    ::= { ip6AdmIfEntry 15 }

    ip6AdmIfDnsAssign OBJECT-TYPE
        SYNTAX      INTEGER {
            off(1),
            self(2),
            other-ns(3)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "Specifies DNS-propagation-policy via RA for this interface.
             For DHCPv6-case, if no 'ip6DhcpDnsServerOpt' entries match,
             this setting also applies as fall-back.
                 off       - Disable DNS-propagation
                 self      - Propagate own (prefix-associated) address
                 other_ns  - Propagate learned or configured name-servers"
        DEFVAL { off }
    ::= { ip6AdmIfEntry 16 }

    ip6AdmIfAdvInterval OBJECT-TYPE
        SYNTAX      INTEGER (0..65535)
        UNITS       "seconds"
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "Time in seconds between unsolicited Router Advertisements
             (see MaxRtrAdvInterval in RFC-4861).
             Relevant only if ip6AdmIfAdvertise is set to 'publish'.
             Used also for calculating advertised router-lifetime unless
             explicitly specified by ip6AdmIfValidLifetime."
        DEFVAL { 600 }
    ::= { ip6AdmIfEntry 17 }

    ip6AdmIfDhcpAddrMode OBJECT-TYPE
        SYNTAX      INTEGER {
            auto(1),
            never(2),
            always(3)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "Specifies how DHCPv6-client decides whether to request own
             IPv6-address for DHCPv6-server or not.
             - auto (default) : Request if managed bit of ip6IfDhcpOffer is set.
             - never : Never request own address.
             - always : Always request own address.
             This field is relevant only if ip6AdmIfDhcpMode is client."
        DEFVAL { auto }
    ::= { ip6AdmIfEntry 18 }

-- End: Administrative: IPv6-interfaces


-- Start: Administrative: Static IPv6-addresses

    ip6AdmAddrTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF Ip6AdmAddrEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table contains administratively configured
             (static) IPv6-unicast-addresses."
    ::= { ip6Config 3 }

    ip6AdmAddrEntry OBJECT-TYPE
        SYNTAX      Ip6AdmAddrEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "A static address entry."
        INDEX   { ip6AdmAddrAddress,
                  ip6AdmAddrLength,
                  ip6AdmAddrIfIndex }
    ::= { ip6AdmAddrTable 1 }

    Ip6AdmAddrEntry ::= SEQUENCE {
        ip6AdmAddrAddress       InetAddressIPv6,
        ip6AdmAddrLength        INTEGER,
        ip6AdmAddrIfIndex       InterfaceIndex,
        ip6AdmAddrAdminStatus   INTEGER,
        ip6AdmAddrLkPrefixId    INTEGER
    }

    ip6AdmAddrAddress OBJECT-TYPE
        SYNTAX     InetAddressIPv6
        MAX-ACCESS read-write
        STATUS     current
        DESCRIPTION
            "The destination IPv6 address of this route.
             This object may not take a multicast address value."
    ::= { ip6AdmAddrEntry 1 }

    ip6AdmAddrLength OBJECT-TYPE
        SYNTAX      INTEGER (1..128)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Indicates the prefix length of the address."
    ::= { ip6AdmAddrEntry 2 }

    ip6AdmAddrIfIndex OBJECT-TYPE
        SYNTAX      InterfaceIndex
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Indicates the Interface the address belongs to."
    ::= { ip6AdmAddrEntry 3 }

    ip6AdmAddrAdminStatus OBJECT-TYPE
        SYNTAX      INTEGER {
            enable(1),
            disable(2),
            delete(3)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The Administration state of the address entry."
        DEFVAL { enable }
    ::= { ip6AdmAddrEntry 4 }

    ip6AdmAddrLkPrefixId OBJECT-TYPE
        SYNTAX      INTEGER
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Referenece to ipLinkPrefixTable entries, used in order to merge
             the static (host) part of this address with the associated
             (probably dynamic) link prefix."
    ::= { ip6AdmAddrEntry 5 }

-- End: Administrative: Static IPv6-addresses


-- Start: Administrative: IPv6-prefixes

    ip6AdmPrefixTable OBJECT-TYPE
        SYNTAX  SEQUENCE OF Ip6AdmPrefixEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The list of administratively configured IPv6-address-prefixes."
    ::= { ip6Config 4 }

    ip6AdmPrefixEntry OBJECT-TYPE
        SYNTAX      Ip6AdmPrefixEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "An interface entry containing objects of
             a particular IPv6 address prefix."
        INDEX   { ip6AdmPrefixId }
    ::= { ip6AdmPrefixTable 1 }

    Ip6AdmPrefixEntry ::= SEQUENCE {
         ip6AdmPrefixId             Unsigned32,
         ip6AdmPrefixIfIndex        InterfaceIndexOrZero,
         ip6AdmPrefixAdminStatus    INTEGER,
         ip6AdmPrefixType           INTEGER,
         ip6AdmPrefixPrefix         InetAddressIPv6,
         ip6AdmPrefixLength         INTEGER,
         ip6AdmPrefixNextHop        InetAddressIPv6,
         ip6AdmPrefixSlaac          INTEGER,
         ip6AdmPrefixDescr          DisplayString
    }

    ip6AdmPrefixId OBJECT-TYPE
        SYNTAX      Unsigned32
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "A unique non-zero value identifying the particular IPv6 prefix."
        DEFVAL { 0 }
    ::= { ip6AdmPrefixEntry 1 }

    ip6AdmPrefixIfIndex OBJECT-TYPE
        SYNTAX      InterfaceIndexOrZero
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "A unique non-zero value identifying
             the particular IPv6 interface or relay."
    ::= { ip6AdmPrefixEntry 2 }

    ip6AdmPrefixAdminStatus OBJECT-TYPE
        SYNTAX      INTEGER {
            enable(1),
            disable(2),
            delete(3)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The administrative status of this entry."
        DEFVAL { enable }
    ::= { ip6AdmPrefixEntry 3 }

    ip6AdmPrefixType OBJECT-TYPE
        SYNTAX      INTEGER {
            dynamic(1),
            static(2)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The type of the prefix associated with the this interface."
        DEFVAL { static }
    ::= { ip6AdmPrefixEntry 4 }

    ip6AdmPrefixPrefix OBJECT-TYPE
        SYNTAX      InetAddressIPv6
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The prefix associated with the this interface."
    ::= { ip6AdmPrefixEntry 5 }

    ip6AdmPrefixLength OBJECT-TYPE
        SYNTAX      INTEGER (0..128)
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The length of the prefix (in bits)."
        DEFVAL { 64 }
    ::= { ip6AdmPrefixEntry 6 }

    ip6AdmPrefixNextHop OBJECT-TYPE
        SYNTAX      InetAddressIPv6
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The next hop associated with this prefix."
    ::= { ip6AdmPrefixEntry 7 }

    ip6AdmPrefixSlaac OBJECT-TYPE
        SYNTAX  INTEGER {
            eui64(1),
--          privacyy-ext(2),
            disabled(3)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "SLAAC method, this setting determines the address format which
             should be the result of the address autoconfiguration procedure
                 eui64          EUI-64-Identifier according RFC 4291
                 privacy-ext    Privacy Extensions according RFC 4941
                 disabled       no SLAAC"
        DEFVAL { disabled }
    ::= { ip6AdmPrefixEntry 8 }

    ip6AdmPrefixDescr OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "User defined description of this prefix entry"
    ::= { ip6AdmPrefixEntry 9 }

-- End: Administrative: IPv6-prefixes


-- Start: Administrative: Static IPv6-routes

    ip6AdmRouteTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF Ip6AdmRouteEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "IPv6 Routing table. This table contains an entry
             for each valid IPv6 unicast route that can be
             used for packet forwarding determination."
    ::= { ip6Config 5 }

    ip6AdmRouteEntry OBJECT-TYPE
        SYNTAX      Ip6AdmRouteEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "A administrative routing entry."
        INDEX   { ip6AdmRouteTarget,
                  ip6AdmRouteTargetLen,
                  ip6AdmRouteSource,
                  ip6AdmRouteSourceLen,
                  ip6AdmRoutePriority }
    ::= { ip6AdmRouteTable 1 }

    Ip6AdmRouteEntry ::= SEQUENCE {
        ip6AdmRouteTarget       InetAddressIPv6,
        ip6AdmRouteTargetLen    INTEGER,
        ip6AdmRouteSource       InetAddressIPv6,
        ip6AdmRouteSourceLen    INTEGER,
        ip6AdmRouteIfIndex      InterfaceIndex,
        ip6AdmRouteAdminStatus  INTEGER,
        ip6AdmRouteNextHop      InetAddressIPv6,
        ip6AdmRoutePriority     INTEGER,
        ip6AdmRouteDescr        DisplayString
    }

    ip6AdmRouteTarget OBJECT-TYPE
        SYNTAX      InetAddressIPv6
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The destination IPv6 address of this route.
             This object may not take a Multicast address value."
    ::= { ip6AdmRouteEntry 1 }

    ip6AdmRouteTargetLen OBJECT-TYPE
        SYNTAX      INTEGER(0..128)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Indicates the prefix length of the destination address."
    ::= { ip6AdmRouteEntry 2 }

    ip6AdmRouteSource OBJECT-TYPE
        SYNTAX      InetAddressIPv6
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The Source IPv6 address of this route.
             This object may not take a Multicast address value."
    ::= { ip6AdmRouteEntry 3 }

    ip6AdmRouteSourceLen OBJECT-TYPE
        SYNTAX      INTEGER(0..128)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Indicates the prefix length of the source address."
        DEFVAL { 64 }
    ::= { ip6AdmRouteEntry 4 }

    ip6AdmRouteIfIndex OBJECT-TYPE
        SYNTAX      InterfaceIndex
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The index value which uniquely identifies the local
             interface through which the next hop of this
             route should be reached.  The interface identified
             by a particular value of this index is the same
             interface as identified by the same value of
             ip6DeviceIfIndex. For routes of the discard type this
             value can be zero."
    ::= { ip6AdmRouteEntry 5 }

    ip6AdmRouteNextHop OBJECT-TYPE
        SYNTAX      InetAddressIPv6
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "On remote routes, the address of the next hop, otherwise set to ::"
    ::= { ip6AdmRouteEntry 6 }

    ip6AdmRouteAdminStatus OBJECT-TYPE
        SYNTAX      INTEGER {
            enable(1),
            disable(2),
            delete(3)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The administrative status of this routing entry."
        DEFVAL { enable }
    ::= { ip6AdmRouteEntry 7 }

    ip6AdmRoutePriority OBJECT-TYPE
        SYNTAX      INTEGER (0..16)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The routing priority for this route,
             a value of 0 means highest priority,
             a value of 16 lowest priority."
        DEFVAL { 8 }
    ::= { ip6AdmRouteEntry 8 }

    ip6AdmRouteDescr OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "User defined description of this routing entry"
    ::= { ip6AdmRouteEntry 9 }

-- End: Administrative: Static IPv6-routes


-- Start: Administrative/Operative: IPv6-link-prefixes

    ip6LinkPrefixTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF Ip6LinkPrefixEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table lists the administratively configured
             downstream-link-prefixes"
    ::= { ip6Config 6 }

    ip6LinkPrefixEntry OBJECT-TYPE
        SYNTAX      Ip6LinkPrefixEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "A link prefix entry."
        INDEX   { ip6LinkPrefixIfIndex,
                  ip6LinkPrefixId }
    ::= { ip6LinkPrefixTable 1 }

    Ip6LinkPrefixEntry ::= SEQUENCE {
        ip6LinkPrefixIfIndex                InterfaceIndex,
        ip6LinkPrefixPrefixId               INTEGER,
        ip6LinkPrefixAdminStatus            INTEGER,
        ip6LinkPrefixSiteLevelValue         Unsigned32,
        ip6LinkPrefixLength                 INTEGER,
        ip6LinkPrefixOnLinkFlag             INTEGER,
        ip6LinkPrefixAutonomousFlag         INTEGER,
        ip6LinkPrefixAdvValidLifetime       Unsigned32,
        ip6LinkPrefixAdvPreferredLifetime   Unsigned32,
        ip6LinkPrefixPrefix                 InetAddressIPv6,
        ip6LinkPrefixOperStatus             INTEGER,
        ip6LinkPrefixId                     INTEGER,
        ip6LinkPrefixSlaac                  INTEGER,
        ip6LinkPrefixAdvertise              INTEGER,
        ip6LinkPrefixDescr                  DisplayString,
        ip6LinkPrefixAdmPrefix              InetAddressIPv6,
        ip6LinkPrefixDhcpOptionSetId        INTEGER,
        ip6LinkPrefixOpValidLifetime        Unsigned32,
        ip6LinkPrefixOpPreferredLifetime    Unsigned32
    }

    ip6LinkPrefixIfIndex OBJECT-TYPE
        SYNTAX      InterfaceIndex
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Interface on which the prefix will published."
    ::= { ip6LinkPrefixEntry 1 }

    ip6LinkPrefixPrefixId OBJECT-TYPE
        SYNTAX      INTEGER
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Ident of the associated administrative prefix,
             see ip6AdmPrefixEntryTable."
    ::= { ip6LinkPrefixEntry 2 }

    ip6LinkPrefixAdminStatus OBJECT-TYPE
        SYNTAX      INTEGER {
            enable(1),
            disable(2),
            delete(3)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Administrative status of this entry."
        DEFVAL { enable }
    ::= { ip6LinkPrefixEntry 3 }

    ip6LinkPrefixSiteLevelValue OBJECT-TYPE
        SYNTAX      Unsigned32
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "SiteLevel Identifier SLA."
        DEFVAL { 0 }
    ::= { ip6LinkPrefixEntry 4 }

    ip6LinkPrefixLength OBJECT-TYPE
        SYNTAX      INTEGER (48..64)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The Length of the new generated prefix,"
        DEFVAL { 64 }
    ::= { ip6LinkPrefixEntry 5 }

    ip6LinkPrefixOnLinkFlag OBJECT-TYPE
        SYNTAX      INTEGER {
            true(1),
            false(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "true: prefix can be used for on-link determination"
        DEFVAL { false }
    ::= { ip6LinkPrefixEntry 6 }

    ip6LinkPrefixAutonomousFlag OBJECT-TYPE
        SYNTAX      INTEGER {
            true(1),
            false(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Determines whether the Autonomous address configuration flag
             within the Router Advertisement Message (RFC 4861) should be set
             or not.
             true:  set the flag - stateless configuration possible
             false: don't set the flag - no autoconfiguration possible"
        DEFVAL { true }
    ::= { ip6LinkPrefixEntry 7 }

    ip6LinkPrefixAdvValidLifetime OBJECT-TYPE
        SYNTAX      Unsigned32
        UNITS       "seconds"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Configured maximum 'valid lifetime'. More details are
             described for field 'ip6LinkPrefixOpValidLifetime'."
        DEFVAL { 2592000 }
    ::= { ip6LinkPrefixEntry 8 }

    ip6LinkPrefixAdvPreferredLifetime OBJECT-TYPE
        SYNTAX      Unsigned32
        UNITS       "seconds"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Configured maximum 'prefered lifetime'. More details are
             described for field 'ip6LinkPrefixOpPreferredLifetime'."
        DEFVAL { 604800 }
    ::= { ip6LinkPrefixEntry 9 }

    ip6LinkPrefixPrefix OBJECT-TYPE
        SYNTAX      InetAddressIPv6
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The current used prefix for this link."
    ::= { ip6LinkPrefixEntry 10 }

    ip6LinkPrefixOperStatus OBJECT-TYPE
        SYNTAX      INTEGER {
            up(1),
            down(2)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The operational status."
        DEFVAL { down }
    ::= { ip6LinkPrefixEntry 11 }

    ip6LinkPrefixId OBJECT-TYPE
        SYNTAX      INTEGER
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Unique ID of this table-entry."
    ::= { ip6LinkPrefixEntry 12 }

    ip6LinkPrefixSlaac OBJECT-TYPE
        SYNTAX      INTEGER {
            eui64(1),
--          privacy-ext(2),
            disabled(3)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "SLAAC method, this setting determines the address format which
             should be the result of the address autoconfiguration procedure
                 eui64          EUI-64-Identifier according RFC 4291
                 privacy-ext    Privacy Extensions according RFC 4941
                 disabled       no SLAAC"
        DEFVAL { eui64 }
    ::= { ip6LinkPrefixEntry 13 }

    ip6LinkPrefixAdvertise OBJECT-TYPE
        SYNTAX      INTEGER {
            enabled(1),
            disabled(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Specifies the propagation policy for this entry when sending
             a Router Advertisement (RA) via the associated interface
                 enabled        include prefix in RA
                 disabled       ignore this prefix when sending RA's"
        DEFVAL { enabled }
    ::= { ip6LinkPrefixEntry 14 }

    ip6LinkPrefixDescr OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "User defined description of this link prefix entry"
    ::= { ip6LinkPrefixEntry 15 }

   ip6LinkPrefixAdmPrefix OBJECT-TYPE
        SYNTAX      InetAddressIPv6
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The configured prefix for this link - without any reference
             to a general prefix."
    ::= { ip6LinkPrefixEntry 16 }

    ip6LinkPrefixDhcpOptionSetId OBJECT-TYPE
        SYNTAX      INTEGER (0..65535)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "If not set to 0 and if 'ip6DhcpOptionSet' entry with matching 'Id'
             value exists then internal DHCPv6-server uses this prefix for
             building requested IA_NA options (see 22.4 and 22.6 of RFC-3315).
             In this case, 'ip6LinkPrefixOnLinkFlag' must be set to 'true' and
             'ip6LinkPrefixAutonomousFlag' must be set to 'false' for proper
             configuration."
    ::= { ip6LinkPrefixEntry 17 }

    ip6LinkPrefixOpValidLifetime OBJECT-TYPE
        SYNTAX      Unsigned32
        UNITS       "seconds"
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "'Operative' valid lifetime. If 'ip6LinkPrefixPrefixId'
             does NOT refer to valid 'ip6Prefix' then lifetime is set
             to configured 'ip6LinkPrefixAdvValidLifetime'. Otherwise,
             it is set to minimum of configured value and inherited
             'ip6PrefixAdvValidLifetime'."
    ::= { ip6LinkPrefixEntry 18 }

    ip6LinkPrefixOpPreferredLifetime OBJECT-TYPE
        SYNTAX      Unsigned32
        UNITS       "seconds"
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "'Operative' preferred lifetime. If 'ip6LinkPrefixPrefixId'
             does NOT refer to valid 'ip6Prefix' then lifetime is set
             to configured 'ip6LinkPrefixAdvPreferredLifetime'. Otherwise,
             it is set to minimum of configured value and inherited
             'ip6PrefixAdvPreferredLifetime'."
    ::= { ip6LinkPrefixEntry 19 }

-- End: Administrative/Operative: IPv6-link-prefixes


