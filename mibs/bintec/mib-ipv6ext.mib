    ::= { ip6RelayEntry 21 }

    ip6RelayOperStatus OBJECT-TYPE
        SYNTAX      INTEGER {
            up(1),
            down(2),
            update(3),
            authenticate(4),
            failed(5)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The current operational status of this tunnel."
        DEFVAL { down }
    ::= { ip6RelayEntry 22 }

