	    SYNTAX  INTEGER {
		disabled(0),
		enabled(1)
            }
	    MAX-ACCESS  read-only
	    STATUS  current
	    DESCRIPTION
		"Early media support for outgoing calls.
		 'disabled': internal phones hear local generated ringback tone
		 'enabled': internal phones get media (RTP) before call is finally
		            answered, if provider sends early media."
	::= { voipProviderProfileEntry 26 }

	voipProviderProfileCLIR OBJECT-TYPE
	    SYNTAX  BITS {
		display(0),
		user(1),
		domain(2),
		privacy-header(3),
		privacy-user(4),
		privacy-id(5)
            }
	    MAX-ACCESS  read-only
	    STATUS  current
	    DESCRIPTION
		"Type of number suppression in outgoing SIP calls:
		 display        : anonymous in display of the From header field.
		 user           : anonymous in user (account) field in the From
		                  header field.
		 domain         : anonymous.invalid in domain of the From header
		                  field.
		 privacy-header : Privacy header field with value 'header'.
		 privacy-user   : Privacy header field with value 'user'.
		 privacy-id     : Privacy header field with value 'id'."
	::= { voipProviderProfileEntry 27 }

	voipProviderProfileSendFromDisplay OBJECT-TYPE
	    SYNTAX  INTEGER {
		none(1),
		username(2),
		clip(3),
		clip-billing(4)
	    }
	    MAX-ACCESS  read-only
	    STATUS  current
	    DESCRIPTION
		"Assignment of source information element to be used in
		 SIP From header display for outgoing call.
		 none: no display used
		 username: the username, same as in Contact header
		 clip: calling party number that should be shown on destination device
		 clip-billing: calling party number for billing (must be a valid number)"
	::= { voipProviderProfileEntry 28 }

	voipProviderProfileSendFromUser OBJECT-TYPE
	    SYNTAX	INTEGER {
		username(1),
		clip(2),
		clip-billing(3)
	    }
	    MAX-ACCESS  read-only
	    STATUS  current
	    DESCRIPTION
		"Assignment of source information element to be used in
		 SIP From header as username for outgoing call.
		 For definition of values see voipSipProvExtSendFromDisplay."
	::= { voipProviderProfileEntry 29 }

	voipProviderProfileSendPPreferred OBJECT-TYPE
	    SYNTAX	INTEGER {
		none(1),
		username(2),
		clip(3),
		clip-billing(4)
	    }
	    MAX-ACCESS  read-only
	    STATUS  current
	    DESCRIPTION
		"Assignment of source information element to be used in
		 SIP P-Preferred-Identity header for outgoing call.
		 For definition of values see voipSipProvExtSendFromDisplay."
	::= { voipProviderProfileEntry 30 }

	voipProviderProfileSendPAsserted OBJECT-TYPE
	    SYNTAX	INTEGER {
		none(1),
		username(2),
		clip(3),
		clip-billing(4)
	    }
	    MAX-ACCESS  read-only
	    STATUS  current
	    DESCRIPTION
		"Assignment of source information element to be used in
		 SIP P-Asserted-Identity header for outgoing call.
		 For definition of values see voipSipProvExtSendFromDisplay."
	::= { voipProviderProfileEntry 31 }

	voipProviderProfileCLIPBilling OBJECT-TYPE
	    SYNTAX	DisplayString
	    MAX-ACCESS  read-only
	    STATUS  current
	    DESCRIPTION
		"A valid phone number used if 'clip-billing' is selected. See above"
	::= { voipProviderProfileEntry 32 }

	voipProviderProfileCheckExistingBinding OBJECT-TYPE
	    SYNTAX  BITS {
		fetch(0),
		reject(1)
            }
	    MAX-ACCESS  read-only
	    STATUS  current
	    DESCRIPTION
		"Check for existing binding:
		 'fetch':  fetch existing bindings before new
		           registration
		 'reject': unregister oldest binding from received
		           registration reject."
	::= { voipProviderProfileEntry 33 }

	voipProviderProfileNatPing OBJECT-TYPE
	    SYNTAX  INTEGER {
		disabled(0),
		enabled(1)
            }
	    MAX-ACCESS  read-only
	    STATUS  current
	    DESCRIPTION
		"For external router usage - periodically a message should
		 be sent to keep the NAT bindings open.
		 'disabled': no keep alive messages used
		 'enabled':  periodically a keep alive message is sent."
	    DEFVAL { disabled }
	::= { voipProviderProfileEntry 34 }

	voipProviderProfileOutNumberTranslation OBJECT-TYPE
	    SYNTAX  BITS {
		addAreaCode(0),
		addCountryCode(1),
		replaceInternationalPrefix(2)
            }
	    MAX-ACCESS  read-only
	    STATUS  current
	    DESCRIPTION
		"Number translation for outgoing calls:
		 addAreaCode    : add country code if number doesn't start with
		                  national prefix.
		 addCountryCode : add country code if number doesn't start with
		                  international prefix.
		 replaceInternationalPrefix: replace internation prefix with '+'."
	    DEFVAL { {} }
	::= { voipProviderProfileEntry 35 }

	voipProviderProfileNetCentricServices OBJECT-TYPE
	    SYNTAX  BITS {
		holdRetrieve(0),
		callDeflection(1),
		numberUpdate(2)
            }
	    MAX-ACCESS  read-only
	    STATUS  current
	    DESCRIPTION
		"There supplementary services that are handled inside our PBX
		 only. Some may be propagated to the provider:
		 holdRetrieve   : Send hold and retrieve to the provider
		 callDeflection : incoming calls can be redirected by
		                  '301 moved temporarily'
		 numberUpdate:    If the internal number changes during a call,
		                  the new number can be propagted by sending a
		                  SIP UPDATE message."
	    DEFVAL { {} }
	::= { voipProviderProfileEntry 36 }

	voipProviderProfileGetCalledNumber OBJECT-TYPE
	    SYNTAX	INTEGER {
		toHeader(1),
		requestURI(2)
	    }
	    MAX-ACCESS  read-only
	    STATUS  current
	    DESCRIPTION
		"Get the called number for incoming calls from:
		 'toHeader': get number from To header
		 'requestURI': get number from request URI."
	    DEFVAL { toHeader }
	::= { voipProviderProfileEntry 37 }

	voipProviderProfileAuthName OBJECT-TYPE
	    SYNTAX	DisplayString
	    MAX-ACCESS  read-only
	    STATUS  current
	    DESCRIPTION
		"Some providers allow a pre-defined authentication name."
	::= { voipProviderProfileEntry 38 }

	voipProviderProfileVendorID OBJECT-TYPE
	    SYNTAX	DisplayString
	    MAX-ACCESS  read-only
	    STATUS  current
	    DESCRIPTION
		"Textual ID that identifies a special vendor."
	::= { voipProviderProfileEntry 39 }

