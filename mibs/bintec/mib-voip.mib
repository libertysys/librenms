		voipProviderProfMgmtProxyProt		INTEGER,
		voipProviderProfMgmtProxyIPVer		INTEGER,
		voipProviderProfMgmtSTUNHost		DisplayString,
		voipProviderProfMgmtSTUNPort		INTEGER,
		voipProviderProfMgmtDomain		DisplayString,
		voipProviderProfMgmtFromDomain		DisplayString,
		voipProviderProfMgmtRegType		INTEGER,
		voipProviderProfMgmtRegInterval		INTEGER,
		voipProviderProfMgmtTlsCertCaCheck	INTEGER,
		voipProviderProfMgmtTlsCertHostCheck	INTEGER,

		voipProviderProfMgmtCodecMask		BITS,
		voipProviderProfMgmtCodecOrder		INTEGER,
		voipProviderProfMgmtG726Endian		INTEGER,
		voipProviderProfMgmtSRTP		INTEGER,
		voipProviderProfMgmtSrcIPCheck		INTEGER,
		voipProviderProfMgmtEarlyMedia		INTEGER,
		voipProviderProfMgmtCLIR		BITS,
		voipProviderProfMgmtSendFromDisplay	INTEGER,
		voipProviderProfMgmtSendFromUser	INTEGER,
		voipProviderProfMgmtSendPPreferred	INTEGER,
		voipProviderProfMgmtSendPAsserted	INTEGER,
		voipProviderProfMgmtCLIPBilling		DisplayString,
		voipProviderProfMgmtCheckExistingBinding	BITS,
		voipProviderProfMgmtNatPing		INTEGER,
		voipProviderProfMgmtOutNumberTranslation	BITS,
		voipProviderProfMgmtNetCentricServices	BITS,
		voipProviderProfMgmtGetCalledNumber	INTEGER,
		voipProviderProfMgmtAuthName		DisplayString,
		voipProviderProfMgmtVendorID		DisplayString
