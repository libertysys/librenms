    voipTraceAdminStatus OBJECT-TYPE
	    SYNTAX  INTEGER {
		disabled(1),
		enabled(2),
		delete(3)
	    }
	    MAX-ACCESS  read-write
	    STATUS  current
	    DESCRIPTION
		"Globally set VoIP trace buffer feature.
		 'disabled' no trace active or stop active trace
		 'enabled'  trace buffer is active using voipTraceMode
		 'delete'   delete existing voip trace buffer, voipTraceBufferID
		            will be set to -1 and voipTraceAdminStatus resets to 'disabled'.
		"
	    DEFVAL { disabled }
	::= { voip 26 }

    voipTraceBufferID OBJECT-TYPE
	    SYNTAX  INTEGER
	    MAX-ACCESS  read-only
	    STATUS  current
	    DESCRIPTION
		"If a trace buffer is available the used ID is saved here.
		 In trcBufTable an entry with that ID (trcBufID) exists. If value
		 is -1, no trace buffer is available.
		"
	    DEFVAL { -1 }
	::= { voip 27 }

    voipTraceMode OBJECT-TYPE
	    SYNTAX  INTEGER {
		state1(1),
		state2(2),
		events1(3),
		events2(4),
		sip(5)
	    }
	    MAX-ACCESS  read-write
	    STATUS  current
	    DESCRIPTION
		"Globally set VoIP trace buffer mode.
		 'state1' dump all registrations and calls (snapshot)
		 'state2' dump all registrations, calls, transactions
		          and internal DNS results (snapshot)
		 'events1' trace current state changes and errors
		 'events2' trace current state changes, errors and all SIP messages
		 'sip' trace all sent and reveived SIP messages only
		"
	    DEFVAL { events1 }
	::= { voip 28 }

