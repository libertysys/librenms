	voipProviderProfMgmtIndex OBJECT-TYPE
	    SYNTAX  INTEGER (1..65535)
	    MAX-ACCESS  read-write
	    STATUS  current
	    DESCRIPTION
		"Unique and persistent index for referencing this provider
		 profile entry."
	    DEFVAL { 1 }
	::= { voipProviderProfMgmtEntry 1 }

	voipProviderProfMgmtDescr OBJECT-TYPE
	    SYNTAX  DisplayString
	    MAX-ACCESS  read-write
	    STATUS  current
	    DESCRIPTION
		"Description of this provider profile entry."
	    DEFVAL { "" }
	::= { voipProviderProfMgmtEntry 2 }

	voipProviderProfMgmtType OBJECT-TYPE
	    SYNTAX  INTEGER {
		predefined(1),
		custom(2),
		converted(3),
		delete(10)
	    }
	    MAX-ACCESS  read-write
	    STATUS  current
	    DESCRIPTION
		"Determines the origin of this entry, predefined(1) entries
		 are factory-defined ones whereas custom(2) is used for
		 user-defined profiles. If set to converted(3) it's the
		 result of a translation process from (now legacy) VoIP-related
		 table entries."
	    DEFVAL { predefined  }
	::= { voipProviderProfMgmtEntry 3 }

	voipProviderProfMgmtAccountType OBJECT-TYPE
	    SYNTAX  INTEGER {
		singleNumber(1),
		ddi(2)
	    }
	    MAX-ACCESS  read-write
	    STATUS  current
	    DESCRIPTION
		"This value determines the type of the account type. This is
		 either a single number or a DDI account."
	    DEFVAL { singleNumber  }
	::= { voipProviderProfMgmtEntry 4 }

	voipProviderProfMgmtRegistrarHost OBJECT-TYPE
	    SYNTAX  DisplayString
	    MAX-ACCESS  read-write
	    STATUS  current
	    DESCRIPTION
		"Host address of Registrar, either domain or IP address."
	    DEFVAL { "" }
	::= { voipProviderProfMgmtEntry 5 }

	voipProviderProfMgmtRegistrarPort OBJECT-TYPE
	    SYNTAX  INTEGER (0..65535)
	    MAX-ACCESS  read-write
	    STATUS  current
	    DESCRIPTION
		"Registrar port, e.g. 5060. Default is 0 (NAPTR/SRV is used)."
	    DEFVAL { 0 }
	::= { voipProviderProfMgmtEntry 6 }

	voipProviderProfMgmtRegistrarProt OBJECT-TYPE
	    SYNTAX  INTEGER {
		udp(1),
		tcp(2),
		tls(3),
		auto(4)
	    }
	    MAX-ACCESS  read-write
	    STATUS  current
	    DESCRIPTION
		"Protocol for connection to registrar. If 'auto' is selected,
		 DNS procedures for NAPTR are used."
	    DEFVAL { udp  }
	::= { voipProviderProfMgmtEntry 7 }

	voipProviderProfMgmtRegistrarIPVer OBJECT-TYPE
	    SYNTAX  INTEGER {
		ipv4(1),
		ipv6(2),
		ipv4prefered(3),
		ipv6prefered(4)
	    }
	    MAX-ACCESS  read-write
	    STATUS  current
	    DESCRIPTION
		"Selection of IP protocol.
		 ipv4: only (resolved) IPv4 addresses are used
		 ipv6: only (resolved) IPv6 addresses are used
		 ipv4prefered: if (resolved) IPv4 address is available for the given
		       protocol then it is used, else IPv6 address is used
		       if available
		 ipv6prefered: if (resolved) IPv6 address is available for the
		       given protocol then it is used, else IPv4 address is used
		       if available"
	    DEFVAL { ipv4  }
	::= { voipProviderProfMgmtEntry 8 }

	voipProviderProfMgmtProxyHost OBJECT-TYPE
	    SYNTAX  DisplayString
	    MAX-ACCESS  read-write
	    STATUS  current
	    DESCRIPTION
		"Host address of Proxy, either domain or IP address. If empty,
		 the registrar host is used."
	    DEFVAL { "" }
	::= { voipProviderProfMgmtEntry 9 }

	voipProviderProfMgmtProxyPort OBJECT-TYPE
	    SYNTAX  INTEGER (0..65535)
	    MAX-ACCESS  read-write
	    STATUS  current
	    DESCRIPTION
