	voipProviderProfileRegistrationType OBJECT-TYPE
	    SYNTAX  INTEGER {
		none(1),
		single(2),
		bulk(3)
	    }
	    MAX-ACCESS  read-only
	    STATUS  current
	    DESCRIPTION
		"Type of registration:
		 none: no registration, remote registrar has static information
		 single: registration of just one account
		 bulk: bulk number registration (bnc)."
	::= { voipProviderProfileEntry 17 }

	voipProviderProfileRegistrationInterval OBJECT-TYPE
	    SYNTAX  INTEGER (1..86400)
	    MAX-ACCESS  read-only
	    STATUS  current
	    DESCRIPTION
		"Registration expire time in seconds."
	::= { voipProviderProfileEntry 18 }


	voipProviderProfileTlsCertCaCheck OBJECT-TYPE
	    SYNTAX  INTEGER {
		disabled(0),
		enabled(1)
            }
	    MAX-ACCESS  read-only
	    STATUS  current
	    DESCRIPTION
		"SRTP support:
		 'disabled': no verification of peer certificate at all
		 'enabled': peer certificate is checked against local CAs."
	::= { voipProviderProfileEntry 19 }

	voipProviderProfileTlsCertHostCheck OBJECT-TYPE
	    SYNTAX  INTEGER {
		disabled(0),
		enabled(1)
            }
	    MAX-ACCESS  read-only
	    STATUS  current
	    DESCRIPTION
		"SRTP support:
		 'disabled': no check for peer certificate's host name
		 'enabled': check peer certificate's host name with DNS host."
	::= { voipProviderProfileEntry 20 }

	voipProviderProfileCodecMask OBJECT-TYPE
	    SYNTAX  BITS {
		g711a(0),
		g711u(1),
		g722(2),
		g729(3),
		g729e(4),
		g726v32(5),
		g726v16(6),
		g726v24(7),
		g726v40(8),
		g723v63(9),
		g723v53(10),
		iLBCv15(11),
		iLBCv13(12),
		dtmf(13),
		t38(14),
		data(15)
